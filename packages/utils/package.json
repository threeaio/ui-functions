{
  "name": "@threeaio/utils",
  "version": "0.6.0",
  "description": "> TODO: description",
  "author": "Nikolaj Sokolowski",
  "homepage": "https://github.com/threeaio/ui-functions#readme",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/threeaio/ui-functions.git"
  },
  "scripts": {
    "test": "jest",
    "build": "tsup",
    "clean": "rimraf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "build:examples": "pnpm run build && mkdir -p examples && cp src/examples/*.html examples/ && cp src/examples/*.css examples/ && mkdir -p examples/js && cp -r dist/* examples/js/",
    "serve:examples": "npx http-server examples -o -c-1"
  },
  "bugs": {
    "url": "https://github.com/threeaio/ui-functions/issues"
  },
  "gitHead": "5465e59abc12e9b579d6b8ad344f82a18420bf25",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./math": {
      "types": "./dist/math/index.d.ts",
      "import": "./dist/math/index.js",
      "require": "./dist/math/index.cjs"
    },
    "./geom": {
      "types": "./dist/geom/index.d.ts",
      "import": "./dist/geom/index.js",
      "require": "./dist/geom/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "math": [
        "./dist/math/index.d.ts"
      ],
      "geom": [
        "./dist/geom/index.d.ts"
      ],
      "types": [
        "./dist/types/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "http-server": "^14.1.1"
  }
}
